generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notification {
  messageID String   @id @default(uuid())
  content   String
  status    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
